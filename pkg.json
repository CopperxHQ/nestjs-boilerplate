{
  "name": "nestjs-starter",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build -c nest-cli.json",
    "start": "nest start",
    "dev": "nest start --watch",
    "start:dev": "nest start --watch -c nest-cli.json",
    "start:debug": "nest start --debug --watch -c nest-cli.json",
    "start:prod": "node dist/main",
    "command": "node dist/commander",
    "command:dev": "ts-node src/commander",
    "scheduler:dev": "nest start --c nest-cli.scheduler.json --watch",
    "scheduler:debug": "nest start --c nest-cli.scheduler.json --debug --watch",
    "scheduler:prod": "node dist/scheduler",
    "migration:run": "ts-node --project tsconfig.json node_modules/typeorm/cli.js -f ormconfig migration:run",
    "migration:generate": "npm run migration:run && ts-node --project tsconfig.json node_modules/typeorm/cli.js -f ormconfig migration:generate -d src/database/migrations -n",
    "migration:create": "ts-node --project tsconfig.json node_modules/typeorm/cli.js -f ormconfig migration:create -d src/database/migrations -n",
    "migration:revert": "ts-node --project tsconfig.json node_modules/typeorm/cli.js -f ormconfig migration:revert",
    "db:query": "ts-node --project tsconfig.json node_modules/typeorm/cli.js -f ormconfig query",
    "fix": "npm run format && npm run lint",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "format": "prettier --write .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "engines": {
    "node": ">=16",
    "npm": ">=8",
    "yarn": "use npm!"
  },
  "dependencies": {
    "@metamask/eth-sig-util": "^4.0.1",
    "@nanogiants/nestjs-swagger-api-exception-decorator": "^1.6.1",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^2.0.1",
    "@nestjs/core": "^8.0.0",
    "@nestjs/jwt": "^8.0.1",
    "@nestjs/passport": "^8.2.2",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/schedule": "^2.0.1",
    "@nestjs/swagger": "^5.2.1",
    "@nestjs/terminus": "^8.1.0",
    "@nestjs/typeorm": "^8.0.3",
    "@supercharge/promise-pool": "^2.3.0",
    "airtable": "^0.11.4",
    "async-retry": "^1.3.3",
    "bottleneck": "^2.19.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dotenv": "^16.0.1",
    "envalid": "^7.3.1",
    "express-ctx": "^0.1.1",
    "express-rate-limit": "^6.4.0",
    "helmet": "^5.1.0",
    "lodash": "^4.17.21",
    "nanoid": "^4.0.0",
    "nest-commander": "^2.5.0",
    "nest-winston": "^1.6.2",
    "nestjs-typeorm-paginate": "^3.2.1",
    "passport-http-header-strategy": "^1.1.0",
    "passport-jwt": "^4.0.0",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "slugify": "^1.6.5",
    "source-map-support": "^0.5.21",
    "swagger-ui-express": "^4.4.0",
    "utility-types": "^3.10.0",
    "web3": "^1.7.4"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "27.5.0",
    "@types/lodash": "^4.14.182",
    "@types/node": "^16.0.0",
    "@types/passport": "^1.0.9",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.0.3",
    "passport-custom": "^1.1.1",
    "prettier": "^2.3.2",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.1",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.0.0",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
